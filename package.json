{
  "name": "vaude-touch",
  "version": "0.1.0",
  "private": true,
  "main": "public/Main.js",
  "homepage": "./",
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "NODE_ENV=development electron .",
    "preelectron-pack": "yarn run build",
    "electron-pack": "build --win --macos -c.extraMetadata.main=build/Main.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "nonExistentID",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": ">=1",
    "jest-dom": "^3.1.3",
    "lint-staged": ">=8",
    "prettier": "1.16.4",
    "react-testing-library": "^6.0.3",
    "sinon": "^7.3.1",
    "typescript": "^3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
